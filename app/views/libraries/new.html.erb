<% provide(:title, 'Make Library') %>
<h1>Make a New Library</h1>

<div class="row">
  <div class="span6">
    <%= form_for(@library) do |f| %>
      <%= render 'shared/library_error_messages' %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <p>
        <%= f.submit "Save", class: "btn btn-large btn-primary" %>
        <%= link_to  "Cancel", current_user , class: "btn btn-large btn-primary" %>
      </p>
    <% end %>
  </div>
</div>
